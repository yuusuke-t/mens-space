<div class="index-container">
	<div class="row">
		<div class="col-xs-1"></div>
		<p>編集</p>
	</div>
	<div class="col-xs-3"></div>
	<div class="col-xs-6">
	<%= form_with(model: @post, local: true) do |f| %>
		<h4>image</h4>
		<%= f.attachment_field :image, placeholder: "プロフィール画像" %>
		<h4>title</h4>
		<%= f.text_field :title %>
		<h4>text</h4>
		<%= f.text_area :text %>

		<%= f.submit "変更を保存" %>
	<% end %>
	</div>
</div>